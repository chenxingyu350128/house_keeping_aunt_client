<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yinafjz.dragon" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>houseKeeper</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <hook src="../node_modules/vue-cli-plugin-cordova/serve-config-hook.js" type="after_prepare" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="19" />
    <platform name="android">
        <icon density="ldpi" src="res/icon/android/ldpi/ic_launcher.png" />
        <icon density="mdpi" src="res/icon/android/mdpi/ic_launcher.png" />
        <icon density="hdpi" src="res/icon/android/hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/icon/android/xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/icon/android/xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/icon/android/xxxhdpi/ic_launcher.png" />
        <preference name="AutoHideSplashScreen" value="true" />
        <preference name="SplashScreenDelay" value="3000" />
        <preference name="FadeSplashScreen" value="true" />
        <preference name="FadeSplashScreenDuration" value="750" />
        <preference name="SplashMaintainAspectRatio" value="false" />
        <preference name="SplashShowOnlyFirstTime" value="false" />
        <config-file parent="/manifest" target="AndroidManifest.xml" xmlns:android="http://schemas.android.com/apk/res/android">
            <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
        </config-file>
        <feature name="Toast">
            <param name="android-package" value="nl.xservices.plugins.Toast" />
        </feature>
        <splash density="land-hdpi" src="res/screen/android/hdpi.png" />
        <splash density="land-ldpi" src="res/screen/android/ldpi.png" />
        <splash density="land-mdpi" src="res/screen/android/mdpi.png" />
        <splash density="land-xhdpi" src="res/screen/android/xhdpi.png" />
        <splash density="land-xxhdpi" src="res/screen/android/xxhdpi.png" />
        <splash density="land-xxxhdpi" src="res/screen/android/xxxhdpi.png" />
        <splash density="port-hdpi" src="res/screen/android/hdpi.png" />
        <splash density="port-ldpi" src="res/screen/android/ldpi.png" />
        <splash density="port-mdpi" src="res/screen/android/mdpi.png" />
        <splash density="port-xhdpi" src="res/screen/android/xhdpi.png" />
        <splash density="port-xxhdpi" src="res/screen/android/xxhdpi.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/xxxhdpi.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-app-exit" spec="0.0.1" />
    <plugin name="cordova-plugin-appversion" spec="^1.0.0" />
    <plugin name="cordova-plugin-camera" spec="^4.1.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-file" spec="^6.0.2" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.3" />
    <plugin name="cordova-plugin-vha-fileopener2" spec="^0.2019.0" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <plugin name="cordova-plugin-x-toast" spec="^2.7.2" />
    <plugin name="jpush-phonegap-plugin" spec="^3.7.3">
	<plugin name="phonegap-plugin-barcodescanner" spec="^8.1.0"/>
        <variable name="APP_KEY" value="014de7bdfde964ad1dddff53" />
        <variable name="CHANNEL" value="developer-default" />
    </plugin>
    <plugin name="cordova-plugin-wechat" spec="^2.9.0">
        <variable name="WECHATAPPID" value="wxed66a2fab7fa7fb8" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.1.0" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="^7.1.4" />
</widget>
